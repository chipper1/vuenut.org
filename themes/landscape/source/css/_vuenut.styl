@font-face {
  font-family: 'Material Icons';
  font-style: normal;
  font-weight: 400;
  src: local('Material Icons'), local('MaterialIcons-Regular'), url(https://fonts.gstatic.com/s/materialicons/v17/2fcrYFNaTjcS6g4U3t-Y5ZjZjT5FdEJ140U2DJYC3mY.woff2) format('woff2');
}
.material-icons {
  font-family: 'Material Icons' !important;
  font-weight: normal;
  font-style: normal;
  font-size: 24px;
  line-height: 1;
  letter-spacing: normal;
  text-transform: none;
  display: inline-block;
  white-space: nowrap;
  word-wrap: normal;
  direction: ltr;
  -webkit-font-feature-settings: 'liga';
  -webkit-font-smoothing: antialiased;
}
.con-menu
  background: rgb(255, 255, 255);
  overflow: hidden;
  position: fixed;
  left: 0px;
  top: 0px;
  width: 100%;
  display:flex;
  align-items: center;
  justify-content: space-between;
  box-shadow: 0px 3px 12px 0px rgba(0, 0, 0, 0.05);
  z-index: 1000;
  .con-img-menu
    width: 150px;
    padding-left: 30px;
    img
       width: 100%;

  ul
    padding-right: 30px;
  li
    float: left;
    margin-left: 5px;
    margin-right: 5px;
    a
      padding: 10px;
      padding-left: 20px;
      padding-right: 20px;
      display: block;
      border-radius: 20px;
      font-weight: bold;
    .current
      background: rgb(secondary)
      color: rgb(255,255,255)

.inicio
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 100px;
  h1
    width: 100%;
    font-size: 40px;
    text-align: center;

.con-centro
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  .vuenut-logo
    width: 300px;

.ulx {
  width: 100%;
  display:flex;
  align-items: center;
  justify-content: center;
  margin-top: 30px;
}
.ulx li {
  font-size: 20px;
  margin: 5px;
  position: relative;
}
.ulx li:hover a {
  padding-right: 60px;
  padding-left: 20px;
  transition: all .3s ease;
  color: rgba(255, 255, 255,0) !important;
}
.ulx li:hover a i {
  color: rgb(255, 255, 255);
  width: 100%;
  border-radius: 25px
}
.ulx li a {
  padding: 10px;
  padding-left: 20px;
  padding-right: 20px;
  background: rgb(primary)
  border-radius: 25px;
  color: rgb(255, 255, 255)
  padding-left: 60px;
  display: block;
  font-size: 16px;
}
.ulx li a i {
  display: block;
  color: inherit;
  background: rgba(0, 0, 0, 0.1);
  width: 41px;
  height: 41px;
  border-radius: 25px;
  position: absolute;
  left: 0px;
  top: 0px;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all .3s ease;
}
.gitbtn a {
  background: rgb(secondary) !important
  // border: 2px solid rgb(primary)
  // color: rgb(primary) !important
}
.induction a{
  background: rgb(secondary) !important
}
